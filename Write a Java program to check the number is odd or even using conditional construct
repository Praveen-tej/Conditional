import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        int n;
        Scanner sc = new Scanner(System.in);
        n=sc.nextInt();
        if (n%2==0){
            System.out.println("EVEN");
        }
        else{
            System.out.println("ODD");
        }
       
    }
}
